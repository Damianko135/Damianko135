<Configuration>
    <Add OfficeClientEdition="64" Channel="MonthlyEnterprise">
        <Product ID="O365ProPlusRetail">
            <Language ID="en-us" /> <!-- English (United States) language pack -->
            <Language ID="nl-nl" /> <!-- Dutch (Netherlands) language pack -->
            
            <!-- Uncomment to exclude apps from installation -->
            <!-- <ExcludeApp ID="Access" /> --> <!-- Exclude Microsoft Access app -->
            <!-- <ExcludeApp ID="Excel" /> --> <!-- Exclude Microsoft Excel app -->
            <!-- <ExcludeApp ID="Groove" /> --> <!-- Exclude Groove Music app -->
            <!-- <ExcludeApp ID="Lync" /> --> <!-- Exclude Skype for Business -->
            <!-- <ExcludeApp ID="OneNote" /> --> <!-- Exclude OneNote app -->
            <!-- <ExcludeApp ID="Outlook" /> --> <!-- Exclude Outlook app -->
            <!-- <ExcludeApp ID="PowerPoint" /> --> <!-- Exclude PowerPoint app -->
            <!-- <ExcludeApp ID="Publisher" /> --> <!-- Exclude Publisher app -->
            <!-- <ExcludeApp ID="Teams" /> --> <!-- Exclude Microsoft Teams app -->
            <!-- <ExcludeApp ID="Word" /> --> <!-- Exclude Word app -->
            <!-- <ExcludeApp ID="SkypeForBusiness" /> --> <!-- Exclude Skype for Business -->
            <!-- <ExcludeApp ID="OneDrive" /> --> <!-- Exclude OneDrive app -->
        </Product>
        
        <!-- Force shutdown apps during install -->
        <Property Name="FORCEAPPSHUTDOWN" Value="TRUE" />
        
        <!-- Automatically activate Office -->
        <!-- <Property Name="AUTOACTIVATE" Value="1" /> -->
        <Property Name="AUTOACTIVATE" Value="0" />
        
        <!-- Shared computer licensing mode -->
        <!-- <Property Name="SharedComputerLicensing" Value="1" /> -->
        
        <!-- Device-based licensing mode -->
        <!-- <Property Name="DeviceBasedLicensing" Value="1" /> -->
        
        <!-- Pin app icons to taskbar -->
        <!-- <Property Name="PinIconsToTaskbar" Value="TRUE" /> -->
    </Add>
    
    <!-- UI settings during install. -->
     <!-- Full, None, HideProgress, Basic -->
    <Display Level="None" AcceptEULA="TRUE" />
    
    <!-- Enable automatic Office updates -->
    <Updates Enabled="TRUE" />
    
    <!-- Standard logging to temp folder -->
    <Logging Level="Standard" Path="%temp%" />
</Configuration>
